<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_hr_social_insurance_tree" model="ir.ui.view">
        <field name="name">hr.social.insurance.tree</field>
        <field name="model">hr.social.insurance</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="employee_id"/>
                <field name="insurance_number"/>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="employee_contribution"/>
                <field name="employer_contribution"/>
                <field name="total_paid_months"/>
                <field name="status"/>
            </tree>
        </field>
    </record>

    <record id="action_hr_social_insurance" model="ir.actions.act_window">
        <field name="name">Social Insurance</field>
        <field name="res_model">hr.social.insurance</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
