<odoo>
    <record id="view_hr_unemployment_insurance_tree" model="ir.ui.view">
        <field name="name">hr.unemployment.insurance.tree</field>
        <field name="model">hr.unemployment.insurance</field>
        <field name="arch" type="xml">
            <tree string="Unemployment Insurance">
                <field name="name"/>
                <field name="employee_id"/>
                <field name="insurance_number"/>
                <field name="contribution_rate"/>
                <field name="benefit_rate"/>
                <field name="contribution_period"/>
                <field name="benefit_duration"/>
                <field name="allowance_amount"/>
                <field name="start_date"/>
                <field name="end_date"/>
                <field name="insurance_org"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="action_hr_unemployment_insurance" model="ir.actions.act_window">
        <field name="name">Unemployment Insurance</field>
        <field name="res_model">hr.unemployment.insurance</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
                Click to create a new Unemployment Insurance record.
            </p>
        </field>
    </record>
</odoo>
